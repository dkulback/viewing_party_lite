
<% if current_user %>
  <div class="log-out">

    <%= link_to 'Log Out', logout_path, method: :delete %>
  <ol class= 'users'>
    <% @users.each do |user|  %>
      <li><%= user.email %></li>
    <% end %>
  </ol>
  </div>
<% else %>
  <div class="new-user">
    <%= button_to 'Register', '/register', method: :get %>
  </div>

  <div class="log-in">
    <%= link_to 'Log In', login_path, method: :get %>
  </div>
<% end %>
